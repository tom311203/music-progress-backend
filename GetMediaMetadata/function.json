{
 "bindings": [
    {
      "authLevel": "function",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [ "get" ],
      "route": "metadata/{sessionId}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "type": "cosmosDB",
      "direction": "in",
      "name": "documents",
      "databaseName": "music-progress-db",
      "containerName": "media-metadata",
      "connectionStringSetting": "music-progress-cosmos_DOCUMENTDB"
    }
  ]
}